    "train_df, test_df = train_test_split(df, test_size=0.3, random_state=42, stratify=df['label'])\n",
    "\n",
    "train_examples = [InputExample(texts=[row['ocr_product'], row['pusat_product']], label=float(row['label'])) for idx, row in train_df.iterrows()]\n",
    "test_examples = [InputExample(texts=[row['ocr_product'], row['pusat_product']], label=float(row['label'])) for idx, row in test_df.iterrows()]\n"
   ]
  },
  {
   "cell_type": "code",
