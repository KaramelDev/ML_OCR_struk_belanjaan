   "metadata": {},
   "outputs": [],
   "source": [
    "model = SentenceTransformer('paraphrase-mpnet-base-v2')\n",
    "\n",
    "train_dataloader = DataLoader(train_examples, shuffle=True, batch_size=16)\n",
    "train_loss = losses.CosineSimilarityLoss(model)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "5663bdd9",
   "metadata": {},
   "outputs": [],
   "source": [
    "sentences1 = [example.texts[0] for example in test_examples]\n",
    "sentences2 = [example.texts[1] for example in test_examples]\n",
    "labels = [example.label for example in test_examples]\n",
    "\n",
    "evaluator = BinaryClassificationEvaluator(sentences1, sentences2, labels)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "6e7d66e6",
   "metadata": {},
   "outputs": [],
   "source": [
    "model.fit(\n",
    "    train_objectives=[(train_dataloader, train_loss)],\n",
    "    evaluator=evaluator,\n",
    "    epochs=3,\n",
    "    warmup_steps=100,\n",
    "    output_path='./fine-tuned-product-mpnet'\n",
    ")\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "4a48e92e",
   "metadata": {},
   "outputs": [],
   "source": [
    "embeddings1 = model.encode(sentences1)\n",
    "embeddings2 = model.encode(sentences2)\n",
    "\n",
    "from sentence_transformers.util import cos_sim\n",
    "predictions = [cos_sim(e1, e2).item() for e1, e2 in zip(embeddings1, embeddings2)]\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "0c5c7239",
   "metadata": {},
   "outputs": [],
   "source": [
    "threshold = 0.6\n",
    "predicted_labels = [1 if p >= threshold else 0 for p in predictions]\n"
   ]
  },
